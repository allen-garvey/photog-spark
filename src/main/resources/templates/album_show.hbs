{{#partial "body"}}
    <h2>{{album.name}}</h2>
    <ul class="thumbnail-list">
        {{#each images}}
            <li class="image-container">
                <a href="{{urlForImage this}}"><img src="{{urlForThumbnail this.thumbnail}}" alt=""/></a>
            </li>
        {{/each}}
    </ul>
{{/partial}}

